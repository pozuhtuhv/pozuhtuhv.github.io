---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}
    {% if page.layout == "default" %}
      {% if page.published != false %}
        <url>
          <loc>{{ site.url }}{{ page.url }}?title={{ page.title }}</loc>
        </url>
      {% endif %}
    {% endif %}
  {% endfor %}
</urlset>